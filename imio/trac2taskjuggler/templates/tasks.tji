flags maintask
flags subtask

{% for prj in prjs.keys()|sort %}
flags {{prj}}
{% endfor %}

{% for prj in prjs.keys()|sort %}
  {% for (mstid, mst) in prjs[prj] %}
task {{mstid}} "{{mst}}" {
  flags maintask
    maxend {{msts[mst]['due']}}
    {% for owner in msts[mst]['own'] %}
  task {{msts[mst]['own'][owner]['t']|join('_')}} "{{owner}}" {
    effort 2h
    allocate sgeulette
    flags subtask
  }
    {% endfor %}
}
  {% endfor %}
{% endfor %}
